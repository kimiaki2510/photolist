<p>良い写真取れましたか?</p>
<%= form_with(model: @record, local: true)do |f|%>
  <div class="form-group">
    <%= f.label :title, 'タイトル'%>
    <%= f.text_field :title %>
  </div>
  <div class="form-group">
    <%= f.label :photo, '写真' %>
    <%= f.file_field :photo %>
  </div>
  <div class="form-group">
    <%= f.label :content, '感想' %>
    <%= f.text_field :content %>
  </div>
  <%= f.submit '投稿する' %>
<% end %>
<%= link_to 'タイムラインに戻る', root_url %>
